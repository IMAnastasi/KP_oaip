{$R *.dfm}
//изначально скрывает веб-браузер
procedure TForm9.FormCreate(Sender: TObject);
begin
webbrowser1.Hide;
end;
//при нажатии на картинку скрывает панель с картинками и показывает веб-браузер с результатами
procedure TForm9.Image10Click(Sender: TObject);
begin
label1.Caption:='Ваш результат ';
panel1.Visible:=false;
webbrowser1.Show;
webbrowser1.Navigate(ExtractFilePath(ParamStr(0))+'web\Игра тип личности\энергетический.htm');
end;
