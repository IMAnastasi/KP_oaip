procedure TForm9.N2Click(Sender: TObject);
begin
 form9.close;
 form7.show;
end;

procedure TForm9.N3Click(Sender: TObject);
begin
form9.close;
form4.show;
end;

procedure TForm9.N4Click(Sender: TObject);
begin
form5.show;
form9.close;
end;


procedure TForm9.N6Click(Sender: TObject);
begin
form6.show;
form9.close;
webbrowser1.Hide;
 panel1.Visible:=true;
end;
 //справка
procedure TForm9.N7Click(Sender: TObject);
begin
ShellExecute(0, PChar('Open'), PChar ('справка.chm'), nil, nil, SW_SHOW);
end;

